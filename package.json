{
    "name": "@noeldemartin/solid-utils",
    "version": "0.6.0",
    "type": "module",
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/noeldemartin-solid-utils.d.ts",
            "default": "./dist/noeldemartin-solid-utils.js"
        },
        "./testing": {
            "types": "./dist/testing.d.ts",
            "default": "./dist/testing.js"
        },
        "./vitest": {
            "types": "./dist/vitest.d.ts",
            "default": "./dist/vitest.js"
        },
        "./chai": {
            "types": "./dist/chai.d.ts",
            "default": "./dist/chai.js"
        }
    },
    "files": [
        "src",
        "dist"
    ],
    "license": "MIT",
    "author": "Noel De Martin",
    "repository": "github:NoelDeMartin/solid-utils",
    "scripts": {
        "build": "vite build",
        "lint": "noeldemartin-lint src",
        "test": "vitest --run",
        "test:ci": "vitest --run --reporter verbose",
        "verify": "noeldemartin-verify"
    },
    "dependencies": {
        "@noeldemartin/utils": "^0.7.0",
        "@rdfjs/types": "^2.0.1",
        "jsonld": "^8.3.3",
        "md5": "^2.3.0",
        "n3": "^1.24.2"
    },
    "devDependencies": {
        "@noeldemartin/eslint-config-typescript": "^0.1.2",
        "@noeldemartin/scripts": "0.3.0-next.88fb0ff21ba1f9015dab043ded20be9518e51248",
        "@noeldemartin/testing": "0.0.0-next.cd489a93da5eaad3f89a4d9ae3734f7ef911b6f6",
        "@tsconfig/node22": "^22.0.0",
        "@types/chai": "^5.2.0",
        "@types/jsonld": "^1.5.15",
        "@types/md5": "^2.3.5",
        "@types/n3": "^1.24.1",
        "@types/node": "^22.13.10",
        "@vitest/expect": "^3.0.9",
        "typescript": "^5.8.2",
        "vite": "^6.2.2",
        "vite-plugin-dts": "4.5.0",
        "vitest": "^3.0.9"
    },
    "eslintConfig": {
        "extends": [
            "@noeldemartin/eslint-config-typescript"
        ]
    },
    "prettier": {
        "printWidth": 120
    }
}
